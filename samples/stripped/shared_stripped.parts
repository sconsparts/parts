Import('*')

env.PartName('shared_stripped')
env.PartVersion('1.0.0')

# This parts file will generate stripped shared object (DLL) from stripped.cpp source code.
# Debugging information for the binary will be completely lost.

env.InstallTarget(
    env.SharedLibrary(
        target = '${PART_NAME}',
        source = 'stripped.cpp',
        NO_STRIP = False
    )
)

