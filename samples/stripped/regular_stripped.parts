Import('*')

env.PartName('regular_stripped')
env.PartVersion('1.0.0')

# This parts file will generate regular stripped executable from stripped.cpp source code.
# Debugging information for the binary will be completely lost.

env.InstallTarget(
    env.Program(
        target = '${PART_NAME}',
        source = 'stripped.cpp',
        NO_STRIP = False
    )
)

