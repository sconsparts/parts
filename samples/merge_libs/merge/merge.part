Import('*')

env.PartName("merge")
env.PartVersion("1.0")

env.DependsOn([Component('A',requires=REQ.SDKLIB),Component('B',requires=REQ.SDKLIB)])

# ideally the env['LIBS'] has what we want. But since the real world
# will probally depend on other stuff so it will want to get the A 
# and B values explicitly not implicitly

a_lib=env.subst("${DEPENDS.A.SDKLIB}")
b_lib=env.subst("${DEPENDS.B.SDKLIB}")

env.Sdk(
env.StaticLibrary("cmod",[a_lib,b_lib])
)
